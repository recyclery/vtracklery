<h1>Listing events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>First at</th>
      <th>Last at</th>
      <th>Wday</th>
      <th>S hr</th>
      <th>S min</th>
      <th>E hr</th>
      <th>E min</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.first_at %></td>
        <td><%= event.last_at %></td>
        <td><%= event.wday %></td>
        <td><%= event.s_hr %></td>
        <td><%= event.s_min %></td>
        <td><%= event.e_hr %></td>
        <td><%= event.e_min %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
